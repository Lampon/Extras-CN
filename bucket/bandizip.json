{
    "version": "7.37",
    "description": "Unzipping software",
    "homepage": "https://www.bandisoft.com/bandizip/",
    "license": "Freeware",
    "url": "https://assets.lampon.cn/apps/Bandizip_7.37_64.7z",
    "hash": "88eee1ecc6b2dc8c6f4ee16fb41af92932b5e770d7ea9adf5f64342250ebee62",
    "bin": [
        "Bandizip.exe"
    ],
    "shortcuts": [
        [
            "Bandizip.exe",
            "Bandizip"
        ]
    ],
    "post_install": [
        "reg add \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\Bandizip\" /v Publisher /d \"423Down\" /f",
        "reg add \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\Bandizip\" /v DisplayName /d \"Bandizip Professional\" /f",
        "reg add \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\Bandizip\" /v DisplayVersion /d \"7.37\" /f",
        "reg add \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\Bandizip\" /v NoRepair /t REG_DWORD /d 1 /f",
        "reg add \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\Bandizip\" /v NoModify /t REG_DWORD /d 1 /f",
        "reg add \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\Bandizip\" /v InstallLocation /d \"$dir\" /f",
        "reg add \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\Bandizip\" /v DisplayIcon /d \"$dir\\Bandizip.exe\" /f",
        "reg add \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\Bandizip\" /v UninstallString /d \"$dir\\Uninstall.exe\" /f",
        "reg add \"HKCU\\Software\\Classes\\*\\shell\\BandizipExtractHere\" /ve /d \"用 Bandizip 解压到当前目录\" /f",
        "reg add \"HKCU\\Software\\Classes\\*\\shell\\BandizipExtractHere\" /v Icon /d \"$dir\\Bandizip.exe\" /f",
        "reg add \"HKCU\\Software\\Classes\\*\\shell\\BandizipExtractHere\\command\" /ve /d \"\"$dir\\Bandizip.exe\" x \"%1\"\" /f",
        "reg add \"HKCU\\Software\\Classes\\*\\shell\\BandizipOpen\" /ve /d \"用 Bandizip 打开\" /f",
        "reg add \"HKCU\\Software\\Classes\\*\\shell\\BandizipOpen\" /v Icon /d \"$dir\\Bandizip.exe\" /f",
        "reg add \"HKCU\\Software\\Classes\\*\\shell\\BandizipOpen\\command\" /ve /d \"\"$dir\\Bandizip.exe\" \"%1\"\" /f"
    ],
    "pre_uninstall": [
        "reg delete \"HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\Bandizip\" /f",
        "reg delete \"HKCU\\Software\\Classes\\*\\shell\\BandizipExtractHere\" /f",
        "reg delete \"HKCU\\Software\\Classes\\*\\shell\\BandizipOpen\" /f"
    ]
}
